{
  "type": "module",
  "name": "pomelogpt-electron",
  "version": "0.1.0",
  "description": "Electron + React (Vite) scaffold for PomeloGPT",
  "main": "electron-main/main.cjs",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"npm:watch\" \"npm:start-electron\"",
    "watch": "vite",
    "start-electron": "cross-env NODE_ENV=development wait-on http://localhost:5173 && electron .",
    "build": "vite build && electron-builder"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "electron": "^30.0.0",
    "electron-builder": "^24.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "lucide-react": "^0.554.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0"
  }
}
